
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>NET CORE 2.x - @ViewBag.Title</title>
    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css">
</head>
<body>
    <nav class="navbar navbar-inverse">
        <nav class="container">
            <a class="navbar-brand" asp-controller="Home" asp-action="Index">NET CORE 2.x</a>
            <ul class="nav navbar-nav">
                <li class="active"><a asp-controller="Home" asp-action="Index">Trang chủ</a></li>
                <li><a asp-controller="Home" asp-action="About">Giới thiệu</a></li>
                <li><a asp-controller="Home" asp-action="Contact">Liên hệ</a></li>
                <li><a asp-controller="Book" asp-action="Index">Sách</a></li>
            </ul>
        </nav>
    </nav>
    <div class="container">
        @RenderBody()
    </div>
    <!-- JQuery -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <!-- Bootstrap JavaScript -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>